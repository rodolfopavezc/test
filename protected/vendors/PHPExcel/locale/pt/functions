##	Statistical functions				Funções estatsticas
##
AVEDEV			= DESV.MDIO			##	Devolve a mdia aritmtica dos desvios absolutos à mdia dos pontos de dados
AVERAGE			= MDIA				##	Devolve a mdia dos respectivos argumentos
AVERAGEA		= MDIAA			##	Devolve uma mdia dos respectivos argumentos, incluindo nmeros, texto e valores lgicos
AVERAGEIF		= MDIA.SE			##	Devolve a mdia aritmtica de todas as clulas num intervalo que cumprem determinado critrio
AVERAGEIFS		= MDIA.SE.S			##	Devolve a mdia aritmtica de todas as clulas que cumprem mltiplos critrios
BETADIST		= DISTBETA			##	Devolve a função de distribuição cumulativa beta
BETAINV			= BETA.ACUM.INV			##	Devolve o inverso da função de distribuição cumulativa relativamente a uma distribuição beta especfica
BINOMDIST		= DISTRBINOM			##	Devolve a probabilidade de distribuição binomial de termo individual
CHIDIST			= DIST.CHI			##	Devolve a probabilidade unicaudal da distribuição qui-quadrada
CHIINV			= INV.CHI			##	Devolve o inverso da probabilidade unicaudal da distribuição qui-quadrada
CHITEST			= TESTE.CHI			##	Devolve o teste para independência
CONFIDENCE		= INT.CONFIANÇA			##	Devolve o intervalo de confiança correspondente a uma mdia de população
CORREL			= CORREL			##	Devolve o coeficiente de correlação entre dois conjuntos de dados
COUNT			= CONTAR			##	Conta os nmeros que existem na lista de argumentos
COUNTA			= CONTAR.VAL			##	Conta os valores que existem na lista de argumentos
COUNTBLANK		= CONTAR.VAZIO			##	Conta o nmero de clulas em branco num intervalo
COUNTIF			= CONTAR.SE			##	Calcula o nmero de clulas num intervalo que corresponde aos critrios determinados
COUNTIFS		= CONTAR.SE.S			##	Conta o nmero de clulas num intervalo que cumprem mltiplos critrios
COVAR			= COVAR				##	Devolve a covariância, que  a mdia dos produtos de desvios de pares
CRITBINOM		= CRIT.BINOM			##	Devolve o menor valor em que a distribuição binomial cumulativa  inferior ou igual a um valor de critrio
DEVSQ			= DESVQ				##	Devolve a soma dos quadrados dos desvios
EXPONDIST		= DISTEXPON			##	Devolve a distribuição exponencial
FDIST			= DISTF				##	Devolve a distribuição da probabilidade F
FINV			= INVF				##	Devolve o inverso da distribuição da probabilidade F
FISHER			= FISHER			##	Devolve a transformação Fisher
FISHERINV		= FISHERINV			##	Devolve o inverso da transformação Fisher
FORECAST		= PREVISÃO			##	Devolve um valor ao longo de uma tendência linear
FREQUENCY		= FREQUÊNCIA			##	Devolve uma distribuição de frequência como uma matriz vertical
FTEST			= TESTEF			##	Devolve o resultado de um teste F
GAMMADIST		= DISTGAMA			##	Devolve a distribuição gama
GAMMAINV		= INVGAMA			##	Devolve o inverso da distribuição gama cumulativa
GAMMALN			= LNGAMA			##	Devolve o logaritmo natural da função gama, Γ(x)
GEOMEAN			= MDIA.GEOMTRICA		##	Devolve a mdia geomtrica
GROWTH			= CRESCIMENTO			##	Devolve valores ao longo de uma tendência exponencial
HARMEAN			= MDIA.HARMNICA		##	Devolve a mdia harmnica
HYPGEOMDIST		= DIST.HIPERGEOM		##	Devolve a distribuição hipergeomtrica
INTERCEPT		= INTERCEPTAR			##	Devolve a intercepção da linha de regressão linear
KURT			= CURT				##	Devolve a curtose de um conjunto de dados
LARGE			= MAIOR				##	Devolve o maior valor k-simo de um conjunto de dados
LINEST			= PROJ.LIN			##	Devolve os parâmetros de uma tendência linear
LOGEST			= PROJ.LOG			##	Devolve os parâmetros de uma tendência exponencial
LOGINV			= INVLOG			##	Devolve o inverso da distribuição normal logartmica
LOGNORMDIST		= DIST.NORMALLOG		##	Devolve a distribuição normal logartmica cumulativa
MAX			= MXIMO			##	Devolve o valor mximo numa lista de argumentos
MAXA			= MXIMOA			##	Devolve o valor mximo numa lista de argumentos, incluindo nmeros, texto e valores lgicos
MEDIAN			= MED				##	Devolve a mediana dos nmeros indicados
MIN			= MNIMO			##	Devolve o valor mnimo numa lista de argumentos
MINA			= MNIMOA			##	Devolve o valor mnimo numa lista de argumentos, incluindo nmeros, texto e valores lgicos
MODE			= MODA				##	Devolve o valor mais comum num conjunto de dados
NEGBINOMDIST		= DIST.BIN.NEG			##	Devolve a distribuição binominal negativa
NORMDIST		= DIST.NORM			##	Devolve a distribuição cumulativa normal
NORMINV			= INV.NORM			##	Devolve o inverso da distribuição cumulativa normal
NORMSDIST		= DIST.NORMP			##	Devolve a distribuição cumulativa normal padrão
NORMSINV		= INV.NORMP			##	Devolve o inverso da distribuição cumulativa normal padrão
PEARSON			= PEARSON			##	Devolve o coeficiente de correlação momento/produto de Pearson
PERCENTILE		= PERCENTIL			##	Devolve o k-simo percentil de valores num intervalo
PERCENTRANK		= ORDEM.PERCENTUAL		##	Devolve a ordem percentual de um valor num conjunto de dados
PERMUT			= PERMUTAR			##	Devolve o nmero de permutações de um determinado nmero de objectos
POISSON			= POISSON			##	Devolve a distribuição de Poisson
PROB			= PROB				##	Devolve a probabilidade dos valores num intervalo se encontrarem entre dois limites
QUARTILE		= QUARTIL			##	Devolve o quartil de um conjunto de dados
RANK			= ORDEM				##	Devolve a ordem de um nmero numa lista numrica
RSQ			= RQUAD				##	Devolve o quadrado do coeficiente de correlação momento/produto de Pearson
SKEW			= DISTORÇÃO			##	Devolve a distorção de uma distribuição
SLOPE			= DECLIVE			##	Devolve o declive da linha de regressão linear
SMALL			= MENOR				##	Devolve o menor valor de k-simo de um conjunto de dados
STANDARDIZE		= NORMALIZAR			##	Devolve um valor normalizado
STDEV			= DESVPAD			##	Calcula o desvio-padrão com base numa amostra
STDEVA			= DESVPADA			##	Calcula o desvio-padrão com base numa amostra, incluindo nmeros, texto e valores lgicos
STDEVP			= DESVPADP			##	Calcula o desvio-padrão com base na população total
STDEVPA			= DESVPADPA			##	Calcula o desvio-padrão com base na população total, incluindo nmeros, texto e valores lgicos
STEYX			= EPADYX			##	Devolve o erro-padrão do valor de y previsto para cada x na regressão
TDIST			= DISTT				##	Devolve a distribuição t de Student
TINV			= INVT				##	Devolve o inverso da distribuição t de Student
TREND			= TENDÊNCIA			##	Devolve valores ao longo de uma tendência linear
TRIMMEAN		= MDIA.INTERNA			##	Devolve a mdia do interior de um conjunto de dados
TTEST			= TESTET			##	Devolve a probabilidade associada ao teste t de Student
VAR			= VAR				##	Calcula a variância com base numa amostra
VARA			= VARA				##	Calcula a variância com base numa amostra, incluindo nmeros, texto e valores lgicos
VARP			= VARP				##	Calcula a variância com base na população total
VARPA			= VARPA				##	Calcula a variância com base na população total, incluindo nmeros, texto e valores lgicos
WEIBULL			= WEIBULL			##	Devolve a distribuição Weibull
ZTEST			= TESTEZ			##	Devolve o valor de probabilidade unicaudal de um teste-z


##
##	Text functions					Funções de texto
##
ASC			= ASC				##	Altera letras ou katakana de largura total (byte duplo) numa cadeia de caracteres para caracteres de largura mdia (byte nico)
BAHTTEXT		= TEXTO.BAHT			##	Converte um nmero em texto, utilizando o formato monetrio ß (baht)
CHAR			= CARCT			##	Devolve o carcter especificado pelo nmero de cdigo
CLEAN			= LIMPAR			##	Remove do texto todos os caracteres não imprimveis
CODE			= CDIGO			##	Devolve um cdigo numrico correspondente ao primeiro carcter numa cadeia de texto
CONCATENATE		= CONCATENAR			##	Agrupa vrios itens de texto num nico item de texto
DOLLAR			= MOEDA				##	Converte um nmero em texto, utilizando o formato monetrio € (Euro)
EXACT			= EXACTO			##	Verifica se dois valores de texto são idênticos
FIND			= LOCALIZAR			##	Localiza um valor de texto dentro de outro (sensvel às maisculas e minsculas)
FINDB			= LOCALIZARB			##	Localiza um valor de texto dentro de outro (sensvel às maisculas e minsculas)
FIXED			= FIXA				##	Formata um nmero como texto com um nmero fixo de decimais
JIS			= JIS				##	Altera letras ou katakana de largura mdia (byte nico) numa cadeia de caracteres para caracteres de largura total (byte duplo)
LEFT			= ESQUERDA			##	Devolve os caracteres mais à esquerda de um valor de texto
LEFTB			= ESQUERDAB			##	Devolve os caracteres mais à esquerda de um valor de texto
LEN			= NM.CARACT			##	Devolve o nmero de caracteres de uma cadeia de texto
LENB			= NM.CARACTB			##	Devolve o nmero de caracteres de uma cadeia de texto
LOWER			= MINSCULAS			##	Converte o texto em minsculas
MID			= SEG.TEXTO			##	Devolve um nmero especfico de caracteres de uma cadeia de texto, a partir da posição especificada
MIDB			= SEG.TEXTOB			##	Devolve um nmero especfico de caracteres de uma cadeia de texto, a partir da posição especificada
PHONETIC		= FONTICA			##	Retira os caracteres fonticos (furigana) de uma cadeia de texto
PROPER			= INICIAL.MAISCULA		##	Coloca em maisculas a primeira letra de cada palavra de um valor de texto
REPLACE			= SUBSTITUIR			##	Substitui caracteres no texto
REPLACEB		= SUBSTITUIRB			##	Substitui caracteres no texto
REPT			= REPETIR			##	Repete texto um determinado nmero de vezes
RIGHT			= DIREITA			##	Devolve os caracteres mais à direita de um valor de texto
RIGHTB			= DIREITAB			##	Devolve os caracteres mais à direita de um valor de texto
SEARCH			= PROCURAR			##	Localiza um valor de texto dentro de outro (não sensvel a maisculas e minsculas)
SEARCHB			= PROCURARB			##	Localiza um valor de texto dentro de outro (não sensvel a maisculas e minsculas)
SUBSTITUTE		= SUBST				##	Substitui texto novo por texto antigo numa cadeia de texto
T			= T				##	Converte os respectivos argumentos em texto
TEXT			= TEXTO				##	Formata um nmero e converte-o em texto
TRIM			= COMPACTAR			##	Remove espaços do texto
UPPER			= MAISCULAS			##	Converte texto em maisculas
VALUE			= VALOR				##	Converte um argumento de texto num nmero
