TRIMMEAN		= MDIA.INTERNA			##	Retorna a mdia do interior de um conjunto de dados
TTEST			= TESTET			##	Retorna a probabilidade associada ao teste t de Student
VAR			= VAR				##	Estima a variância com base em uma amostra
VARA			= VARA				##	Estima a variância com base em uma amostra, inclusive nmeros, texto e valores lgicos
VARP			= VARP				##	Calcula a variância com base na população inteira
VARPA			= VARPA				##	Calcula a variância com base na população total, inclusive nmeros, texto e valores lgicos
WEIBULL			= WEIBULL			##	Retorna a distribuição Weibull
ZTEST			= TESTEZ			##	Retorna o valor de probabilidade uni-caudal de um teste-z


##
##	Text functions			Funções de texto
##
ASC			= ASC				##	Altera letras do inglês ou katakana de largura total (bytes duplos) dentro de uma seqüência de caracteres para caracteres de meia largura (byte nico)
BAHTTEXT		= BAHTTEXT			##	Converte um nmero em um texto, usando o formato de moeda ß (baht)
CHAR			= CARACT			##	Retorna o caractere especificado pelo nmero de cdigo
CLEAN			= TIRAR				##	Remove todos os caracteres do texto que não podem ser impressos
CODE			= CDIGO			##	Retorna um cdigo numrico para o primeiro caractere de uma seqüência de caracteres de texto
CONCATENATE		= CONCATENAR			##	Agrupa vrios itens de texto em um nico item de texto
DOLLAR			= MOEDA				##	Converte um nmero em texto, usando o formato de moeda $ (dlar)
EXACT			= EXATO				##	Verifica se dois valores de texto são idênticos
FIND			= PROCURAR			##	Procura um valor de texto dentro de outro (diferencia maisculas de minsculas)
FINDB			= PROCURARB			##	Procura um valor de texto dentro de outro (diferencia maisculas de minsculas)
FIXED			= DEF.NM.DEC			##	Formata um nmero como texto com um nmero fixo de decimais
JIS			= JIS				##	Altera letras do inglês ou katakana de meia largura (byte nico) dentro de uma seqüência de caracteres para caracteres de largura total (bytes duplos)
LEFT			= ESQUERDA			##	Retorna os caracteres mais à esquerda de um valor de texto
LEFTB			= ESQUERDAB			##	Retorna os caracteres mais à esquerda de um valor de texto
LEN			= NM.CARACT			##	Retorna o nmero de caracteres em uma seqüência de texto
LENB			= NM.CARACTB			##	Retorna o nmero de caracteres em uma seqüência de texto
LOWER			= MINSCULA			##	Converte texto para minsculas
MID			= EXT.TEXTO			##	Retorna um nmero especfico de caracteres de uma seqüência de texto começando na posição especificada
MIDB			= EXT.TEXTOB			##	Retorna um nmero especfico de caracteres de uma seqüência de texto começando na posição especificada
PHONETIC		= FONTICA			##	Extrai os caracteres fonticos (furigana) de uma seqüência de caracteres de texto
PROPER			= PRI.MAISCULA			##	Coloca a primeira letra de cada palavra em maiscula em um valor de texto
REPLACE			= MUDAR				##	Muda os caracteres dentro do texto
REPLACEB		= MUDARB			##	Muda os caracteres dentro do texto
REPT			= REPT				##	Repete o texto um determinado nmero de vezes
RIGHT			= DIREITA			##	Retorna os caracteres mais à direita de um valor de texto
RIGHTB			= DIREITAB			##	Retorna os caracteres mais à direita de um valor de texto
SEARCH			= LOCALIZAR			##	Localiza um valor de texto dentro de outro (não diferencia maisculas de minsculas)
SEARCHB			= LOCALIZARB			##	Localiza um valor de texto dentro de outro (não diferencia maisculas de minsculas)
SUBSTITUTE		= SUBSTITUIR			##	Substitui um novo texto por um texto antigo em uma seqüência de texto
T			= T				##	Converte os argumentos em texto
TEXT			= TEXTO				##	Formata um nmero e o converte em texto
TRIM			= ARRUMAR			##	Remove espaços do texto
UPPER			= MAISCULA			##	Converte o texto em maisculas
VALUE			= VALOR				##	Converte um argumento de texto em um nmero
